import math
print("input x1")
x1 = float(input())
print("input y1")
y1 = float(input())
print("input x2")
x2 = float(input())
print("input y2")
y2 = float(input())
#Примечание: Решим задачу с помощью прямоугольного треугольника.
#Для этого нужно найти два его катета, гипотенуза - искомое расстояние.
katet1 = abs(x1 - x2)
katet2 = abs(y1 - y2)
ansver = math.sqrt(katet1**2 + katet2**2)
print("Ansver is: "+str(ansver))
